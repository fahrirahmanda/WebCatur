            <form method="POST" action="/tambah_peserta">
              <tr>
                <td class="text-center">
                  <select name="Grup" id="Grup" class="form-control " required>
                    <option value="">Pilih Grup</option>
                    <option value="A">A</option>
                    <option value="B">B</option>
                    <option value="C">C</option>
                    <option value="D">D</option>
                  </select>
                </td>
                <td>
                  <select name="Fullname" id="Fullname" class="form-control" required>
                    <option value="">Pilih Peserta</option>
                    <% for(var count=0; count < fullname_data.length;count++) { %>
                    <option value="<%= fullname_data[count].Fullname %>" style="color: black;">
                      <%= fullname_data[count].Fullname %>
                    </option>
                    <% } %>
                  </select>
                </td>
                <td class="text-center">
                  <input type="submit" name="Submit" class="btn btn-primary" value="Tambah" />
                </td>
                <td class="text-center">
                  <% if(typeof errorMessage == "string" && errorMessage.length > 0 ) { %>
                    <button class="btn btn-danger" disabled><%= errorMessage %></button>
                  <% } else if(typeof successMessage == "string" && successMessage.length > 0 ) { %>
                    <button class="btn btn-success" disabled><%= successMessage %></button>
                  <% } else { %>
                    <button class="btn btn-secondary" disabled>Idle</button>
                  <% } %>
                </td>
